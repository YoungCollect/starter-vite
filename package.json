{
  "name": "starter-vite",
  "private": true,
  "version": "0.0.0",
  "description": "A monorepo starter based on vite",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vite build --mode prod",
    "build:lib": "node scripts/build-lib.js",
    "preview": "vite preview --mode dev",
    "format": "npm run format:lint && npm run format:pretty",
    "format:lint": "eslint . --ext .js,.jsx,.vue,.cjs,.mjs --fix --config .eslintrc.cjs --no-eslintrc",
    "format:pretty": "prettier . --write",
    "postpublish": "pnpm build:lib"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/runtime-corejs3": "^7.22.15",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@oneyoung/pino-cli": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "chalk": "^5.6.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^8.0.3",
    "lint-staged": "^11.1.2",
    "magic-string": "^0.30.17",
    "minimist": "^1.2.8",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "3.0.3",
    "rollup-plugin-node-externals": "^7.1.3",
    "vite": "^6.0.5",
    "yorkie-pnpm": "^2.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit"
  },
  "lint-staged": {
    "*.{js,jsx,vue,cjs,mjs}": "eslint --fix --config .eslintrc.cjs --no-eslintrc"
  }
}
